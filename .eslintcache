n } from \"react-i18next\";\nimport { languageSlice } from \"../../redux/language/slice\";\nimport { useHistory } from \"react-router-dom\";\nimport jwt_decode, { JwtPayload as DefaultJwtPayload } from \"jwt-decode\";\nimport { userSlice } from \"../../redux/user/slice\";\nimport { getShoppingCart } from \"../../redux/shoppingCart/slice\";\n\ninterface JwtPayload extends DefaultJwtPayload {\n  username: string;\n}\n\nexport const Header: React.FC = () => {\n  //useSelector is self-defined to decouple store and component\n  //(don't have to specify the type of state anymore)\n  const language = useSelector((state) => state.language.language);\n  const languageList = useSelector((state) => state.language.languageList);\n  const dispatch = useDispatch();\n  const { t } = useTranslation();\n  const history = useHistory();\n\n  const jwt = useSelector((state) => state.user.token);\n  const [username, setUserName] = useState(\"\");\n\n  const shoppingCartItems = useSelector((state) => state.shoppingCart.items);\n  const shoppingCartLoading = useSelector(\n    (state) => state.shoppingCart.loading\n  );\n\n  //update username state and get shopping cart info when use log in\n  // when logged in\n  useEffect(() => {\n    if (jwt !== null) {\n      const token = jwt_decode<JwtPayload>(jwt);\n      setUserName(token.username);\n      dispatch(getShoppingCart(jwt));\n    }\n  }, [jwt]);\n\n  //define language change action and dispatch to reducer\n  const handleLanguageChange = (e) => {\n    dispatch(languageSlice.actions.getLanguage(e.key));\n  };\n\n  //clear token and direct to main page when sign out\n  const handleSignOut = () => {\n    dispatch(userSlice.actions.signOut());\n    history.push(\"/\");\n  };\n\n  return (\n    <div className={styles[\"app-header\"]}>\n      {/* top header */}\n      <div className={styles[\"top-header\"]}>\n        <Typography.Text>{t(\"header.slogan\")}</Typography.Text>\n\n        <Dropdown.Button\n          style={{ marginLeft: 15 }}\n          overlay={\n            <Menu onClick={handleLanguageChange}>\n              {languageList.map((l) => {\n                return <Menu.Item key={l.code}>{l.name}</Menu.Item>;\n              })}\n            </Menu>\n          }\n          icon={<GlobalOutlined />}\n        >\n          {language === \"zh\" ? \"中文\" : \"English\"}\n        </Dropdown.Button>\n\n        {/* display user email, shoppingCart and signOut buttons when log in, \n        else display register and signIn buttons  */}\n        {jwt ? (\n          <Button.Group className={styles[\"button-group\"]}>\n            <span>\n              {t(\"header.welcome\")}\n              <Typography.Text strong>{username}</Typography.Text>\n            </span>\n            <Button\n              loading={shoppingCartLoading}\n              onClick={() => history.push(\"/shoppingCart\")}\n            >\n              {t(\"header.shoppingCart\")}({shoppingCartItems.length})\n            </Button>\n            <Button onClick={handleSignOut}>{t(\"header.signOut\")}</Button>\n          </Button.Group>\n        ) : (\n          <Button.Group className={styles[\"button-group\"]}>\n            <Link to={\"/register\"}>\n              <Button>{t(\"header.register\")}</Button>\n            </Link>\n            <Link to={\"/signIn\"}>\n              <Button>{t(\"header.signin\")}</Button>\n            </Link>\n          </Button.Group>\n        )}\n      </div>\n\n      {/* main header */}\n      <Layout.Header className={styles[\"main-header\"]}>\n        <Link to={\"/\"}>\n          <Typography.Title level={3} className={styles.title}>\n            {t(\"header.title\")}\n          </Typography.Title>\n        </Link>\n        <Input.Search\n          className={styles[\"search-input\"]}\n          placeholder={\"请输入旅游目的地，主题或关键字\"}\n          onSearch={(keywords) => history.push(\"/search/\" + keywords)}\n        />\n      </Layout.Header>\n\n      {/* main menu */}\n      <Menu mode={\"horizontal\"} className={styles[\"main-menu\"]}>\n        <Menu.Item key=\"1\"> {t(\"header.home_page\")} </Menu.Item>\n        <Menu.Item key=\"2\"> {t(\"header.weekend\")} </Menu.Item>\n        <Menu.Item key=\"3\"> {t(\"header.group\")} </Menu.Item>\n        <Menu.Item key=\"6\"> {t(\"header.cruise\")} </Menu.Item>\n        <Menu.Item key=\"7\"> {t(\"header.hotel\")} </Menu.Item>\n        <Menu.Item key=\"8\"> {t(\"header.local\")} </Menu.Item>\n        <Menu.Item key=\"9\"> {t(\"header.theme\")} </Menu.Item>\n        <Menu.Item key=\"12\"> {t(\"header.visa\")} </Menu.Item>\n        <Menu.Item key=\"13\"> {t(\"header.enterprise\")} </Menu.Item>\n        <Menu.Item key=\"14\"> {t(\"header.high_end\")} </Menu.Item>\n        <Menu.Item key=\"15\"> {t(\"header.outdoor\")} </Menu.Item>\n        <Menu.Item key=\"16\"> {t(\"header.insurance\")} </Menu.Item>\n      </Menu>\n    </div>\n  );\n};\n","/Users/Cara/Desktop/projects/travel-app/src/components/productCollection/ProductCollection.tsx",[],"/Users/Cara/Desktop/projects/travel-app/src/components/sideMenu/SideMenu.tsx",[],"/Users/Cara/Desktop/projects/travel-app/src/components/sideMenu/mockup.ts",[],"/Users/Cara/Desktop/projects/travel-app/src/components/productCollection/ProductImage.tsx",[],"/Users/Cara/Desktop/projects/travel-app/src/pages/signIn/SignInPage.tsx",[],"/Users/Cara/Desktop/projects/travel-app/src/pages/register/RegisterPage.tsx",[],"/Users/Cara/Desktop/projects/travel-app/src/pages/detail/DetailPage.tsx",["157"],"import React, { useEffect } from \"react\";\nimport { RouteComponentProps, useParams } from \"react-router-dom\";\nimport {\n  Spin,\n  Row,\n  Col,\n  DatePicker,\n  Divider,\n  Typography,\n  Anchor,\n  Menu,\n  Button,\n} from \"antd\";\nimport styles from \"./DetailPage.module.css\";\nimport { Header } from \"../../components/header/Header\";\nimport { Footer } from \"../../components/footer/Footer\";\nimport { ProductDesc } from \"../../components/productDesc/ProductDesc\";\nimport { ProductComments } from \"../../components/productComments/ProductComments\";\nimport { commentMockData } from \"./mockup\";\nimport { getProductDetail } from \"../../redux/productDetail/slice\";\nimport { useSelector } from \"../../redux/hooks\";\nimport { useDispatch } from \"react-redux\";\nimport { ShoppingCartOutlined } from \"@ant-design/icons\";\nimport { addShoppingCartItem } from \"../../redux/shoppingCart/slice\";\n\ninterface MatchParams {\n  touristRouteId: string;\n}\n\nconst { RangePicker } = DatePicker;\n\nexport const DetailPage: React.FC<RouteComponentProps<MatchParams>> = (\n  props\n) => {\n  const { touristRouteId } = useParams<MatchParams>();\n\n  const loading = useSelector((state) => state.productDetail.loading);\n  const error = useSelector((state) => state.productDetail.error);\n  const product = useSelector((state) => state.productDetail.data);\n\n  const dispatch = useDispatch();\n\n  const jwt = useSelector((state) => state.user.token) as string;\n  const shoppingCartLoading = useSelector(\n    (state) => state.shoppingCart.loading\n  );\n\n  //call product detail url and store it as product\n  useEffect(() => {\n    const fetchData = async () => {\n      dispatch(getProductDetail(touristRouteId));\n    };\n    fetchData();\n  }, []);\n\n  if (loading) {\n    return (\n      <Spin\n        size=\"large\"\n        style={{\n          marginTop: 200,\n          marginBottom: 200,\n          marginLeft: \"auto\",\n          marginRight: \"auto\",\n          width: \"100%\",\n        }}\n      />\n    );\n  }\n\n  if (error) {\n    return <div>{error}</div>;\n  }\n\n  return (\n    <>\n      <Header />\n\n      <div className={styles[\"page-content\"]}>\n        {/* product description and date selection */}\n        <div className={styles[\"product-desc-container\"]}>\n          <Row>\n            <Col span={13}>\n              <ProductDesc\n                title={product.title}\n                description={product.description}\n                price={product.originalPrice}\n                coupons={product.coupons}\n                points={product.points}\n                discount={product.price}\n                rating={product.rating}\n                pictures={product.touristRoutePictures.map((p) => p.url)}\n              />\n            </Col>\n            <Col span={11}>\n              <Button\n                style={{ marginTop: 50, marginBottom: 30, display: \"block\" }}\n                type=\"primary\"\n                danger\n                loading={shoppingCartLoading}\n                onClick={() => {\n                  dispatch(\n                    addShoppingCartItem({ jwt, touristRouteId: product.id })\n                  );\n                }}\n              >\n                <ShoppingCartOutlined />\n                放入购物车\n              </Button>\n              <RangePicker open style={{ marginTop: 20 }} />\n            </Col>\n          </Row>\n        </div>\n\n        {/* anchor menu */}\n        <Anchor className={styles[\"product-detail-anchor\"]}>\n          <Menu mode=\"horizontal\">\n            <Menu.Item key=\"1\">\n              <Anchor.Link href=\"#feature\" title=\"产品特色\"></Anchor.Link>\n            </Menu.Item>\n            <Menu.Item key=\"3\">\n              <Anchor.Link href=\"#fees\" title=\"费用\"></Anchor.Link>\n            </Menu.Item>\n            <Menu.Item key=\"4\">\n              <Anchor.Link href=\"#notes\" title=\"预订须知\"></Anchor.Link>\n            </Menu.Item>\n            <Menu.Item key=\"5\">\n              <Anchor.Link href=\"#comments\" title=\"用户评价\"></Anchor.Link>\n            </Menu.Item>\n          </Menu>\n        </Anchor>\n\n        {/* product features */}\n        <div id=\"feature\" className={styles[\"product-detail-container\"]}>\n          <Divider orientation=\"center\">\n            <Typography.Title level={3}>产品特色</Typography.Title>\n          </Divider>\n          {/* render data in html form get from backend api */}\n          <div\n            dangerouslySetInnerHTML={{ __html: product.features }}\n            style={{ margin: 50 }}\n          ></div>\n        </div>\n\n        {/* fees */}\n        <div id=\"fees\" className={styles[\"product-detail-container\"]}>\n          <Divider orientation={\"center\"}>\n            <Typography.Title level={3}>费用</Typography.Title>\n          </Divider>\n          <div\n            dangerouslySetInnerHTML={{ __html: product.fees }}\n            style={{ margin: 50 }}\n          ></div>\n        </div>\n\n        {/* notice */}\n        <div id=\"note\" className={styles[\"product-detail-container\"]}>\n          <Divider orientation={\"center\"}>\n            <Typography.Title level={3}>预定须知</Typography.Title>\n          </Divider>\n          <div\n            dangerouslySetInnerHTML={{ __html: product.notes }}\n            style={{ margin: 50 }}\n          ></div>\n        </div>\n\n        {/* product comments */}\n        <div id=\"comments\" className={styles[\"product-detail-container\"]}>\n          <Divider orientation={\"center\"}>\n            <Typography.Title level={3}>用户评价</Typography.Title>\n          </Divider>\n          <div style={{ margin: 40 }}>\n            <ProductComments data={commentMockData} />\n          </div>\n        </div>\n      </div>\n\n      <Footer />\n    </>\n  );\n};\n","/Users/Cara/Desktop/projects/travel-app/src/redux/store.ts",["158"],"/Users/Cara/Desktop/projects/travel-app/src/i18n/configs.ts",[],"/Users/Cara/Desktop/projects/travel-app/src/redux/hooks.ts",[],"/Users/Cara/Desktop/projects/travel-app/src/components/productDesc/ProductDesc.tsx",[],"/Users/Cara/Desktop/projects/travel-app/src/pages/detail/mockup.ts",[],"/Users/Cara/Desktop/projects/travel-app/src/components/productComments/ProductComments.tsx",[],"/Users/Cara/Desktop/projects/travel-app/src/redux/productDetail/slice.ts",[],"/Users/Cara/Desktop/projects/travel-app/src/redux/recommendProducts/slice.ts",[],"/Users/Cara/Desktop/projects/travel-app/src/redux/language/slice.ts",[],"/Users/Cara/Desktop/projects/travel-app/src/pages/search/SearchPage.tsx",["159"],"import styles from \"./SearchPage.module.css\";\nimport React, { useEffect } from \"react\";\nimport { Header } from \"../../components/header/Header\";\nimport { Footer } from \"../../components/footer/Footer\";\nimport { ProductList } from \"../../components/productList/ProductList\";\nimport { useParams, useLocation } from \"react-router-dom\";\nimport { Spin } from \"antd\";\nimport { searchProduct } from \"../../redux/productSearch/slice\";\nimport { useSelector } from \"../../redux/hooks\";\nimport { useDispatch } from \"react-redux\";\n\ninterface MatchParams {\n  keywords: string;\n}\n\nexport const SearchPage: React.FC = () => {\n  const { keywords } = useParams<MatchParams>();\n\n  const loading = useSelector((state) => state.productSearch.loading);\n  const error = useSelector((state) => state.productSearch.error);\n  const pagination = useSelector((state) => state.productSearch.pagination);\n  const productList = useSelector((state) => state.productSearch.data);\n\n  const dispatch = useDispatch();\n  const location = useLocation();\n\n  //search tourist routes based on the url change\n  useEffect(() => {\n    dispatch(searchProduct({ nextPage: 1, pageSize: 10, keywords }));\n  }, [location]);\n\n  //call searchProduct() to get new page info\n  const onPageChange = (nextPage, pageSize) => {\n    dispatch(searchProduct({ nextPage, pageSize, keywords }));\n  };\n\n  if (loading) {\n    return (\n      <Spin\n        size=\"large\"\n        style={{\n          marginTop: 200,\n          marginBottom: 200,\n          marginLeft: \"auto\",\n          marginRight: \"auto\",\n          width: \"100%\",\n        }}\n      />\n    );\n  }\n  if (error) {\n    return <div>{error}</div>;\n  }\n\n  return (\n    <>\n      <Header />\n      <div className={styles[\"page-content\"]}>\n        {/* product list */}\n        <div className={styles[\"product-list-container\"]}>\n          <ProductList\n            data={productList}\n            paging={pagination}\n            onPageChange={onPageChange}\n          />\n        </div>\n      </div>\n      <Footer />\n    </>\n  );\n};\n","/Users/Cara/Desktop/projects/travel-app/src/components/productList/ProductList.tsx",[],"/Users/Cara/Desktop/projects/travel-app/src/redux/productSearch/slice.ts",[],"/Users/Cara/Desktop/projects/travel-app/src/layouts/UserLayout.tsx",[],"/Users/Cara/Desktop/projects/travel-app/src/pages/register/RegisterForm.tsx",[],"/Users/Cara/Desktop/projects/travel-app/src/pages/signIn/SignInForm.tsx",["160"],"import { Form, Input, Button, Checkbox } from \"antd\";\nimport styles from \"./SignInForm.module.css\";\nimport { signIn } from \"../../redux/user/slice\";\nimport { useDispatch } from \"react-redux\";\nimport { useSelector } from \"../../redux/hooks\";\nimport { useEffect } from \"react\";\nimport { useHistory } from \"react-router-dom\";\n\n\nconst layout = {\n  labelCol: { span: 8 },\n  wrapperCol: { span: 16 },\n};\n\nconst tailLayout = {\n  wrapperCol: { offset: 8, span: 16 },\n};\n\nexport const SignInForm = () => {\n    const loading = useSelector(state => state.user.loading);\n    const jwt = useSelector(state => state.user.token)\n\n    const dispatch = useDispatch();\n    const history = useHistory();\n\n  const onFinish = (values: any) => {\n    console.log(\"Success:\", values);\n    dispatch(signIn({\n        email: values.username,\n        password: values.password,\n    }))\n  };\n\n  const onFinishFailed = (errorInfo: any) => {\n    console.log(\"Failed:\", errorInfo);\n  };\n\n  //rerender component and direct to main page when jwt changes(user logged in)\n  useEffect(() => {\n      if (jwt !== null) {\n          history.push(\"/\");\n      }\n  }, [jwt])\n\n  return (\n    <Form\n      {...layout}\n      name=\"basic\"\n      initialValues={{ remember: true }}\n      onFinish={onFinish}\n      onFinishFailed={onFinishFailed}\n      className={styles[\"signIn-form\"]}\n    >\n      <Form.Item\n        label=\"Username\"\n        name=\"username\"\n        rules={[{ required: true, message: \"Please input your username!\" }]}\n      >\n        <Input />\n      </Form.Item>\n\n      <Form.Item\n        label=\"Password\"\n        name=\"password\"\n        rules={[{ required: true, message: \"Please input your password!\" }]}\n      >\n        <Input.Password />\n      </Form.Item>\n\n      <Form.Item {...tailLayout} name=\"remember\" valuePropName=\"checked\">\n        <Checkbox>Remember me</Checkbox>\n      </Form.Item>\n\n      <Form.Item {...tailLayout}>\n        <Button type=\"primary\" htmlType=\"submit\" loading={loading}>\n          Submit\n        </Button>\n      </Form.Item>\n    </Form>\n  );\n};\n","/Users/Cara/Desktop/projects/travel-app/src/redux/user/slice.ts",[],"/Users/Cara/Desktop/projects/travel-app/src/pages/shoppingCart/ShoppingCart.tsx",[],"/Users/Cara/Desktop/projects/travel-app/src/redux/shoppingCart/slice.ts",[],"/Users/Cara/Desktop/projects/travel-app/src/components/paymentTable/PaymentTable.tsx",[],"/Users/Cara/Desktop/projects/travel-app/src/pages/placeOrder/PlaceOrderPage.tsx",[],"/Users/Cara/Desktop/projects/travel-app/src/redux/order/slice.ts",[],"/Users/Cara/Desktop/projects/travel-app/src/components/CheckOutDetailForm/CheckOutDetailForm.tsx",[],{"ruleId":"161","replacedBy":"162"},{"ruleId":"163","replacedBy":"164"},{"ruleId":"161","replacedBy":"165"},{"ruleId":"163","replacedBy":"166"},{"ruleId":"167","severity":1,"message":"168","line":44,"column":6,"nodeType":"169","endLine":44,"endColumn":11,"suggestions":"170"},{"ruleId":"167","severity":1,"message":"171","line":54,"column":6,"nodeType":"169","endLine":54,"endColumn":8,"suggestions":"172"},{"ruleId":"173","severity":1,"message":"174","line":44,"column":1,"nodeType":"175","endLine":44,"endColumn":37},{"ruleId":"167","severity":1,"message":"176","line":30,"column":6,"nodeType":"169","endLine":30,"endColumn":16,"suggestions":"177"},{"